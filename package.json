{
  "name": "noodoe-work",
  "version": "1.0.0",
  "description": "Noodoe work",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm i nodemon rimraf npm-run-all",
    "start": "npm-run-all -s clean build start:simple",
    "start:simple": "node .",
    "start:dev": "npm-run-all -s clean build -p watch:*",
    "start:debug": "npm-run-all -s clean build -p watch:* debug",
    "clean": "npm-run-all -p clean:*",
    "clean:dist": "rimraf dist",
    "clean:cov": "rimraf coverage",
    "debug": "node-debug",
    "build": "npm-run-all -s build:ts",
    "build:ts": "./node_modules/typescript/bin/tsc",
    "watch:ts": "./node_modules/typescript/bin/tsc --watch",
    "watch:node": "nodemon --delay 1s",
    "lint": "./node_modules/tslint/bin/tslint ./src/{,**/}*.ts",
    "pretest": "npm-run-all -s clean build",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles --forceExit",
    "test:cov": "jest --coverage --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --forceExit"
  },
  "keywords": [
    "typescript",
    "swagger"
  ],
  "author": "evanlu",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": "^14.17.0"
  },
  "dependencies": {
    "@nestjs/common": "7.6.17",
    "@nestjs/core": "7.6.17",
    "@nestjs/mongoose": "7.2.4",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/schedule": "^0.4.3",
    "axios": "^0.21.1",
    "bluebird": "3.5.3",
    "bson": "4.0.1",
    "cors": "2.8.5",
    "event-dispatch": "0.4.1",
    "glob": "7.1.3",
    "lodash": "^4.17.21",
    "mongodb": "3.2.4",
    "mongoose": "5.12.10",
    "nodemon": "^1.19.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2",
    "tedious": "2.7.1",
    "ts-helpers": "^1.1.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.6.17",
    "@types/bluebird": "3.5.25",
    "@types/bson": "1.0.11",
    "@types/config": "0.0.34",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.11",
    "@types/glob": "7.1.1",
    "@types/jest": "^26.0.23",
    "@types/js-yaml": "3.11.4",
    "@types/lodash": "4.14.119",
    "@types/mongodb": "3.1.19",
    "@types/mongoose": "5.10.5",
    "@types/reflect-metadata": "0.1.0",
    "@types/request": "2.48.5",
    "@types/rimraf": "2.0.4",
    "@types/source-map-support": "0.4.1",
    "jest": "26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "source-map-support": "0.5.10",
    "ts-jest": "^26.5.6",
    "ts-loader": "^4.4.2",
    "ts-node": "7.0.1",
    "tslint": "5.12.1",
    "typescript": "4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
