{
  "extends": "tslint:latest",
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "class-name": true,
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "eofline": true,
    "jsdoc-format": true,
    "member-access": false,
    "whitespace": [
      true,
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "no-any": true,
    "no-unsafe-any": true,
    "curly": false,
    "no-reference": false,
    "no-var-requires": false,
    "no-submodule-imports": false,
    "ordered-imports": false,
    "no-trailing-whitespace": false,
    "object-literal-shorthand": false,
    "interface-name": false,
    "no-implicit-dependencies": false
  },
  "rulesDirectory": []
}

